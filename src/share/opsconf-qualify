_show_help() {
    exitcode=$1
    cat <<EOF
opsconf qualify FILE VERSION: Set the version VERSION of the file FILE as "in qualification".
EOF
    exit $exitcode
}

run_cmd() {
    if [ $# -eq 2 ] ; then
        file=$1
        version=$2
    else
        _show_help 1
    fi

    if ! is_current_branch_qualif ; then
        log_error "Qualifying a file can only be done on the \"$OPSCONF_BRANCH_QUALIF\" branch. Aborting."
        exit 1
    fi

    retrieve_version "$OPSCONF_BRANCH_WORK" "$file" "$version"
}
